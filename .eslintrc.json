{
  "env": {
    "es2021": true,
    "worker": true,
    "serviceworker": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-restricted-globals": ["error", "name", "length"],
    "import/no-unresolved": "off",
    "import/extensions": "off"
  }
}

